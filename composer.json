{
    "name": "computerminds/oiko",
    "description": "Oiko structure data platform",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [{
        "name": "ComputerMinds",
        "email": "info@computerminds.co.uk"
    }],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "arthur-e/wicket",
                "version": "1.3.2",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/arthur-e/Wicket.git",
                    "reference": "1.3.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet.draw",
                "version": "0.3.0",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.draw",
                    "reference": "v0.3.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "1.0.3",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v1.0.3.zip",
                    "type": "zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.git",
                    "reference": "v1.0.3"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-json": "*",
        "arthur-e/wicket": "^1.3.2",
        "composer/installers": "^1.7",
        "computerminds/cidoc-crm": "^1.2.0",
        "cweagans/composer-patches": "^1.7.3",
        "drupal/admin_toolbar": "^3.4",
        "drupal/avatars": "^1.0@beta",
        "drupal/color_field": "^2.0",
        "drupal/composer_deploy": "^1.7",
        "drupal/core-composer-scaffold": "~9.5.0",
        "drupal/core-recommended": "~9.5.0",
        "drupal/css_editor": "^1.2",
        "drupal/csv_serialization": "^1.5",
        "drupal/ctools": "^3.0",
        "drupal/editor_file": "^1.5",
        "drupal/entity_share": "^3.0@beta",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/eva": "^2.0",
        "drupal/field_group": "^3.3",
        "drupal/gdpr": "^3.0@beta",
        "drupal/geocoder": "^2.0",
        "drupal/geofield": "^1.6",
        "drupal/google_analytics": "^2.1",
        "drupal/honeypot": "^1.24",
        "drupal/key_auth": "^2.1",
        "drupal/leaflet": "^2.2",
        "drupal/metatag": "^1.0",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.12",
        "drupal/r4032login": "^2.2",
        "drupal/search_api": "^1.30",
        "drupal/seven": "^1.0",
        "drupal/sharemessage": "^1.0@beta",
        "drupal/squid_autocomplete": "^1.0@alpha",
        "drupal/stable": "^2.0",
        "drupal/upgrade_status": "^4.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_delimited_list": "^2.0@alpha",
        "drupal/views_fields_on_off": "^2.0",
        "drupal/webform": "^6.2",
        "drush/drush": "^11.6",
        "leaflet/leaflet": "1.0.3",
        "leaflet/leaflet.draw": "^0.3.0",
        "oomphinc/composer-installers-extender": "dev-master",
        "php-http/guzzle6-adapter": "^1.1",
        "phpwatch/array_key_first_last-polyfill": "dev-master",
        "wikimedia/composer-merge-plugin": "^2.1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "webroot/"
            }
        },
        "composer-exit-on-patch-failure": true,
        "merge-plugin": {
            "include": [
                "webroot/modules/custom/cidoc_spec/composer.json",
                "webroot/modules/custom/cidoc_sync/composer.json",
                "webroot/modules/custom/edtf/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-types": ["drupal-library"],
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/entity_share": {
                "Allow notification of entity changes": "https://www.drupal.org/files/issues/2020-07-30/entity_share-automatic_synchronization-WIP-3080629-11.patch"
            }
        },
        "composer-version": "^1.8.4|^2.6.6"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/core-composer-scaffold": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true
        }
    }
}
